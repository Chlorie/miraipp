INPUT = @PROJECT_SOURCE_DIR@/lib/include/mirai
EXCLUDE = @PROJECT_SOURCE_DIR@/lib/include/mirai/detail
EXCLUDE_SYMBOLS = mpp::detail
RECURSIVE = YES
OUTPUT_LANGUAGE = Chinese
